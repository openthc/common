{
	"name": "openthc/common",
	"description": "OpenTHC Common Library",
	"license": "GPL-3.0-only",
	"autoload": {
		"exclude-from-classmap": [
			"vendor/erusev/parsedown-extra/test"
		],
		"files": [
			"lib/err.php",
			"lib/php.php",
			"lib/stat.php"
		],
		"psr-4": {
			"OpenTHC\\": "./lib"
		}
	},
	"bin": [
		"bin/openthc-cfg"
	],
	"require": {
		"php": ">=7.3.0",
		"ext-curl": "*",
		"ext-date": "*",
		"ext-json": "*",
		"ext-session": "*",
		"ext-sockets": "*",
		"ext-PDO": "*",
		"erusev/parsedown-extra": "0.8.1",
		"firebase/php-jwt": "6.3",
		"giggsey/libphonenumber-for-php": "8.12.49",
		"guzzlehttp/guzzle": "6.5.8",
		"slim/slim": "3.12.3",
		"slim/twig-view": "2.5.1",
		"twig/twig": "2.15.3",
		"edoceo/radix": "^2021.0"
	},
	"require-dev": {
		"behat/behat": "3.10.0",
		"php-webdriver/webdriver": "1.12.1",
		"phpmd/phpmd": "2.12.0",
		"phpstan/phpstan": "1.7.8",
		"phpunit/phpunit": "9.5.20",
		"sebastian/phpcpd": "6.0.3"
	}
}
