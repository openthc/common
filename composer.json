{
	"name": "openthc/common",
	"description": "OpenTHC Common Library",
	"license": "MIT",
	"autoload": {
		"exclude-from-classmap": [
			"vendor/erusev/parsedown-extra/test"
		],
		"files": [
			"lib/err.php",
			"lib/php.php",
			"lib/stat.php"
		],
		"psr-4": {
			"OpenTHC\\": "./lib",
			"OpenTHC\\Test\\": "./test"
		}
	},
	"require": {
		"php": ">=8.2.20",
		"ext-curl": "*",
		"ext-date": "*",
		"ext-json": "*",
		"ext-session": "*",
		"ext-sockets": "*",
		"ext-PDO": "*",
		"docopt/docopt": "1.0.6",
		"edoceo/lowline": "v2024.0.309",
		"edoceo/radix": "2021.0.0",
		"erusev/parsedown-extra": "0.8.1",
		"geoip2/geoip2": "3.2.0",
		"firebase/php-jwt": "6.11.1",
		"giggsey/libphonenumber-for-php": "9.0.10",
		"guzzlehttp/guzzle": "7.9.3",
		"league/oauth2-client": "2.8.1",
		"slim/slim": "4.14.0",
		"swaggest/json-schema": "0.12.43"
	},
	"require-dev": {
		"php-webdriver/webdriver": "1.15.2",
		"phpmd/phpmd": "2.15.0",
		"phpstan/phpstan": "2.1.19",
		"phpunit/phpunit": "11.5.27"
	}
}
